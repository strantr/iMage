{
	"compilerOptions": {
		"module": "System",
		"lib": ["ES2015", "DOM", "DOM.Iterable"],
		"target": "ES2020",
		"strict": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictPropertyInitialization": true
	}
}
